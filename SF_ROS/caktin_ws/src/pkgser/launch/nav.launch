  <!-- Map server -->
  <arg name="map_file" default="$(find pkgser)/maps/maplocal.yaml"/>
  <node name="map_server" pkg="map_server" type="map_server" args="$(arg map_file)" />

  <arg name="initial_pose_x" default="-0.0661779327588"/> <!-- Use 17.0 for willow's map in simulation -->
  <arg name="initial_pose_y" default="-0.0111327251006"/> <!-- Use 17.0 for willow's map in simulation -->
  <arg name="initial_pose_a" default="0.0640466510723"/>

  <include file="$(find pkgser)/launch/includes/amcl.launch.xml">
<!--  -->
    <!-- <arg name="initial_pose_x" value="1.0"/> -->
    <!-- <arg name="initial_pose_y" value="1.0"/> -->
    <!-- <arg name="initial_pose_a" value="0"/> -->


    <arg name="initial_pose_x" value="$(arg initial_pose_x)"/>
    <arg name="initial_pose_y" value="$(arg initial_pose_y)"/>
    <arg name="initial_pose_a" value="$(arg initial_pose_a)"/>

  </include>

  <include file="$(find pkgser)/launch/includes/move_base.launch.xml"/>

</launch>

